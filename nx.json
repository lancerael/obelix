{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build"]
      }
    }
  },
  "namedInputs": {
    "dist": ["!{projectRoot}/**/dist/**"]
  },
  "targetDefaults": {
    "build": {
      "inputs": ["default", "^default"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["^build"]
    }
  }
}